{

    "loadchrome":{
    "description": "Load Test Page",
    "action": "loadpage",
    "params": {
      "url": "http://localhost:5000//",
      "browser": "chrome",
      "options": [
        "--user-data-dir=C:\\Users\\Josep\\AppData\\Local\\Google\\Chrome\\User Data\\Default",
        "--profile-directory=Default"
        ],
        "experimentalOptions": [
          {
          "key":"excludeSwitches",
          "value": ["enable-automation"]
        },
        {
          "key": "useAutomationExtension",
          "value": false
        }
      ],
      "wait": 2
    }
  },

  "loadedge":   {
    "description": "Load Test Page",
    "action": "loadpage",
    "params": {
      "url": "http://localhost:5000/",
      "browser": "edge",
      "options": [
        "--user-data-dir=C:\\Users\\Josep\\AppData\\Local\\Microsoft\\Edge\\User Data\\Default",
        "--profile-directory=Default"
        ],
        "experimentalOptions": [
          {
          "key":"excludeSwitches",
          "value": ["enable-automation"]
        },
        {
          "key": "useAutomationExtension",
          "value": false
        }
      ],
      "wait": 2
    }
  },

  "enter_name" : {
    "description": "type in name",
    "action": "settext",
    "params": {
      "objectname": "txt_name",
      "value": {"id": "row1", "col": "name"}
    },
    "wait":2,
    "skip": false
  },

  "enter_email" : {
    "description": "type in email",
    "action": "settext",
    "params": {
      "objectname": "txt_email",
      "value": {"col": "email"}
    },
    "wait":2,
    "skip": false
  },

  "enter_age" : {
    "description": "type in age",
    "action": "settext",
    "params": {
      "objectname": "txt_age",
      "value": {"id": "*", "col": "age"}
    },
    "wait":2,
    "skip": false
  }
